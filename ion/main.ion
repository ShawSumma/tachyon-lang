bench = import('std/benchmark')
type = extern('std/print')
factorial(fn) = {
  x = fn
  ret = x
  loop{
    x -= 1
    ret += x
    if x <= 1{
      ret
    }
  }
}
x = 1
benchmark_factorial = bench.time(factorial)
loop{
  print('calculating '+str(5**x))
  result = benchmark_factorial(5**x)
  print('')
  x += 1
  if x > 7{
    0
  }
}
